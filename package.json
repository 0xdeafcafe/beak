{
  "name": "beak",
  "description": "A feathery cross-platform API crafting tool",
  "private": true,
  "author": {
    "name": "Alexander Forbes-Reed (0xdeafcafe)",
    "email": "info@getbeak.app"
  },
  "homepage": "https://getbeak.app",
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "**/electron-builder",
      "**/electron-builder/**"
    ]
  },
  "engines": {
    "yarn": "^1.4.2"
  },
  "resolutions": {
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2"
  },
  "scripts": {
    "build": "lerna run build --stream",
    "build:web": "lerna run build --stream --scope '@beak/{share,website}'",
    "build:website": "lerna run build --stream --scope @beak/website",
    "build:share": "lerna run build --stream --scope @beak/share",
    "prepackage": "yarn build",
    "prepackage:release": "yarn build",
    "package": "lerna run package --stream --scope @beak/electron-host",
    "package:release": "lerna run package:release --stream --scope @beak/electron-host",
    "start": "lerna exec --parallel --concurrency 1 --stream --scope '@beak/{app,electron-host}' yarn start",
    "start:screenshot": "SCREENSHOT_SIZING=true lerna exec --parallel --concurrency 1 --stream --scope '@beak/{app,electron-host}' yarn start",
    "start:website": "lerna exec --parallel --concurrency 1 --stream --scope '@beak/website' yarn start",
    "start:share": "lerna exec --parallel --concurrency 1 --stream --scope '@beak/share' yarn start",
    "test": "jest",
    "lint": "eslint --ext .tsx --ext .ts './packages/'",
    "typecheck": "lerna run typecheck --stream",
    "unfuck": "lerna clean && yarn --force",
    "postinstall": "node ./node-module-overrides/index.js",
    "release-safety": "yarn typecheck && yarn lint && yarn build"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.3",
    "@types/sanitize-html": "^2.6.2",
    "@types/semver": "^7.3.12",
    "@types/url-parse": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-react": "^2.0.1",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.21.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "lerna": "^5.4.3",
    "nx": "^14.5.7",
    "react-spring": "^9.5.2",
    "sanitize-html": "^2.7.1",
    "semver": "^7.3.7",
    "ts-md5": "^1.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.8",
    "vite-plugin-sentry": "^1.1.4"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.4.5",
    "@sentry/cli": "^2.5.2",
    "@sentry/electron": "^4.0.0",
    "@sentry/react": "^7.11.1",
    "@sentry/tracing": "^7.11.1",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "hw-fingerprint": "^2.1.0",
    "jest": "^27.5.1",
    "keytar": "^7.9.0",
    "monaco-editor": "^0.34.0",
    "pretty-bytes": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "tslog": "^3.3.3",
    "url-parse": "^1.5.10",
    "valid-filename": "^4.0.0",
    "vm2": "^3.9.10"
  }
}
